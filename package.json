{
  "name": "symfony-app-template",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build:prod": "encore production",
    "build:dev": "encore dev",
    "dev-server": "encore dev-server --hot --public http://symfony-app-template.vm:8080 --host 0.0.0.0 --disable-host-check",
    "lint": "$npm_execpath run lint:js; $npm_execpath run lint:css",
    "lint:js": "eslint assets tests/cypress tests/jest --ext .js,.vue --fix",
    "lint:css": "stylelint 'assets/**/*.scss' --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "browserslist": [
    "> 0.5%",
    "not ie 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@symfony/webpack-encore": "^0.28.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "@yproximite/eslint-config-yprox": "^1.3.15",
    "@yproximite/stylelint-config-wordpress": "^1.1.0",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "core-js": "^3.6.0",
    "cypress": "^3.6.1",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-cypress": "^2.7.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^9.4.2",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "sass": "^1.23.3",
    "sass-loader": "^8.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-prettier": "^1.1.1",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "@sentry/browser": "^5.10.2",
    "@sentry/integrations": "^5.10.2",
    "date-fns": "^2.7.0",
    "jquery": "^3.4.1",
    "vue": "^2.6.11"
  }
